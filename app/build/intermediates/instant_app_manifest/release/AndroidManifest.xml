<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jungyoonsung.startingpoint"
    android:targetSandboxVersion="2"
    android:versionCode="13"
    android:versionName="10.0.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="32" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:extractNativeLibs="false"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.jungyoonsung.startingpoint.SchoolSettings.SchoolSettings" />
        <activity
            android:name="com.jungyoonsung.startingpoint.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.jungyoonsung.startingpoint.provider"
            android:exported="false"
            android:grantUriPermissions="true" >

            <!-- ressource file to create -->
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/path" >
            </meta-data>
        </provider>
        <provider
            android:name="androidx.lifecycle.ProcessLifecycleOwnerInitializer"
            android:authorities="com.jungyoonsung.startingpoint.lifecycle-process"
            android:exported="false"
            android:multiprocess="true" />
    </application>

</manifest>